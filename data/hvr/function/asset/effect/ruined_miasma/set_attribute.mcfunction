#> hvr:asset/effect/ruined_miasma/set_attribute
#
# Set attribute
#
# @within function
#   hvr:asset/effect/ruined_miasma/given
#   hvr:asset/effect/ruined_miasma/regiven

# Add attribute
    attribute @s max_health modifier add hvr:asset/effect/ruined_miasma.max_health -0.25 add_multiplied_total
    attribute @s armor modifier add hvr:asset/effect/ruined_miasma.armor -0.25 add_multiplied_total
    attribute @s armor_toughness modifier add hvr:asset/effect/ruined_miasma.armor_toughness -0.25 add_multiplied_total
    attribute @s attack_damage modifier add hvr:asset/effect/ruined_miasma.attack_damage -0.25 add_multiplied_total
    attribute @s movement_speed modifier add hvr:asset/effect/ruined_miasma.movement_speed -0.25 add_multiplied_total